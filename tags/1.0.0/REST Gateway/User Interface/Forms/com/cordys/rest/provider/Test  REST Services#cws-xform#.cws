function btInvoke_Click(eventObject)
{
    if (validateData())
    {
        url = ipRestURI.getValue();
        if (application && window.application.organization)
        {
            addParameterToURL("organization", window.application.organization);
        }
        var rr = new RestfulResource(url);

        if (httpMethodType == "GET")
        {
            rr.retrieve();
        }
        else
            if (httpMethodType == "PUT")
            {
                if (getParsed())
                {
                    rr.create(taRequest.getValue());
                }
            }
            else
                if (httpMethodType == "POST")
                {
                    if (getParsed())
                    {
                        rr.update(taRequest.getValue());
                    }
                }
                else
                {
                    rr.remove();
                }
    }
}

function validateData()
{
    if (ipRestURI.getValue() != "" && tbRestServices.getRows().length != 0)
    {
        return true;
    }
    else
    {
        return false;
    }
}

function addParameterToURL(name, value)
{
    var gatewayName = "/com.cordys.web.rest.RESTGateway.wcp";
    url = baseURL + url;
    url += gatewayName;
    var questionMarkPosition = url.indexOf("?");
    var addAmpersand = questionMarkPosition >= 0 && questionMarkPosition < url.length - 1 && !/&$/.test(url);
    var addQuestionMark = questionMarkPosition <= 0;
    if (addQuestionMark)
    {
        url += "?";
    }
    if (addAmpersand)
    {
        url += "&";
    }
    url += name + "=" + value;
}

function trim(str)
{
    return str.replace(/^\s+|\s+$/g, "");
}

function getParsed()
{
    requestSOAP = cordys.loadXMLDocument(taRequest.getValue());
    if (!trim(taRequest.getValue()))
    {
        application.showError(localization_get("blankRequest"));
        return false;
    }
    else
        if (requestSOAP.parseError.reason != "")
        {
            application.showError(requestSOAP.parseError.reason);
            return false;
        }
        else
        {
            return true;
        }
}


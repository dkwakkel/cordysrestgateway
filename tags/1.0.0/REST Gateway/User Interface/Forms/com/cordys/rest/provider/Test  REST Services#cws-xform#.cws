function tbRestServices_OnSelectRow(eventObject)
{
    clearData();
    businessObject = cordys.getEventTarget(eventObject).businessObject;
    uri = cordys.getNodeText(businessObject, ".//*[local-name()='URIPattern']");
    httpMethodType = cordys.getNodeText(businessObject, ".//*[local-name()='HttpMethod']");
    composeRequest(uri, httpMethodType);
}

function composeRequest(uri, httpMethodType)
{
    if (resourceName != cordys.getNodeText(businessObject, ".//*[local-name()='Resource']"))
    {
        resourceName = cordys.getNodeText(businessObject, ".//*[local-name()='Resource']");
        key = resourceKeyPath + resourceName + ".xml";
        callResourceXMLTemplate();
        displayBodyXML(httpMethodType);
    }
    else
    {
        displayBodyXML(httpMethodType);
    }
    ipRestURI.setValue(uri);
}

function callResourceXMLTemplate()
{
    GetXMLObjectModel.reset();
}

function displayBodyXML(httpMethodType)
{
    if (httpMethodType == "POST" || httpMethodType == "PUT")
    {
        taRequest.enable();
        toGetRestBody("HTTPBody");
    }
    else
    {
        taRequest.disable();
        taRequest.setValue("");
    }
}

function toGetRestBody(xPathNodeText)
{
    var xPathForSoapBody = ".//*[local-name()='" + httpMethodType + "']/*[local-name()='WSOperation'][@URIPattern='" + uri + "']/*[local-name()='" + xPathNodeText + "']";
    var someNode = cordys.selectXMLNode(resourceTemplateData, xPathForSoapBody);
	if(someNode)
	{
		taRequest.setValue(xmlutil1.xml2nicestring(someNode.firstChild));
	}
	else
	{
		application.showError(localization_get("invalidMapping"));
	}
}

function clearData()
{
    taRequest.setValue("");
    taResponse.setValue("");
    ipRestURI.setValue("");

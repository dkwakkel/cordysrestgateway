function initGlobalObjects()
{
    var nsPrefixes = {
        'SOAP' : 'http://schemas.xmlsoap.org/soap/envelope/',
        'ldap' : 'http://schemas.cordys.com/1.0/ldap',
        'store' : 'http://schemas.cordys.com/1.0/xmlstore'
    };
    globalObj = new Object();
    globalObj.nsPrefixes = nsPrefixes;
    globalObj.constants = new Object();
    globalObj.ISV = "isv";
    globalObj.ORG = "org";
    globalObj.SOAPXPath = "/SOAP:Envelope/SOAP:Body";
    var userDn = system.getUser().authUserDN;
    globalObj.instanceDN = userDn.substring(userDn.lastIndexOf("cn=cordys"));
    globalObj.wsdlGatewayURL = "/cordys/com.eibus.web.tools.wsdl.WSDLGateway.wcp";
    globalObj.curLabeledUri = "";
    globalObj.curDefinition = null;
    globalObj.curMethodDNs = new Array();
    globalObj.XMLSTORE_PATH = "/Cordys/servicemappings/rest/";
    globalObj.resourcePath = null;
    globalObj.resourceLastModified = null;
    // WSDLManager
    manager = document.createElement("div");
    manager.id = "manager";
    manager.style.display = "none";
    application.appendToBody(manager);
    application.addType(manager, "wcp.library.util.wsdl.WSDLManager");
    manager.setResolveExternals(true);
    manager.onerror = "showError()";
    // XMLUtil
    xmlUtil = document.createElement("div");
    xmlUtil.id = "xmlUtil";
    xmlUtil.style.display = "none";
    application.appendToBody(xmlUtil);
    application.addType(xmlUtil, "wcp.library.util.XMLUtil");
}

function GetSoftwarePackagesOperationModel_OnRequest(eventObject)
{
    var baseXPath = globalObj.SOAPXPath + "/ldap:GetSoftwarePackages";
    var request = eventObject.request;
    cordys.setXMLNamespaces(request, globalObj.nsPrefixes);
    cordys.setNodeText(request, baseXPath + "/ldap:dn", globalObj.instanceDN);
    cordys.setNodeText(request, baseXPath + "/ldap:sort", "ascending");
}

function GetSoftwarePackagesOperationModel_OnResponse(eventObject)
{
    var baseXPath = globalObj.SOAPXPath + "/ldap:GetSoftwarePackagesResponse";
    var orgDnXPath = baseXPath + "/ldap:tuple/ldap:old/ldap:entry/@dn";
    var orgDescXPath = baseXPath + "/ldap:tuple/ldap:old/ldap:entry/ldap:cn/ldap:string";
    var response = eventObject.response;
    cordys.setXMLNamespaces(response, globalObj.nsPrefixes);
    var isvDNs = cordys.selectXMLNodes(response, orgDnXPath);
    var isvDescs = cordys.selectXMLNodes(response, orgDescXPath);
    for ( var i = 0; i < isvDescs.length; i++)
    {
        var option = {
            value : cordys.getTextContent(isvDNs[i]),
            description : cordys.getTextContent(isvDescs[i])
        };
        selPackagesOrg.addOption(option, false, 0, false);
    }
}


function btDownload_Click(eventObject)
{
    if (tbRestServices.getRows().length > 0)
    {
        var downloadResource = cordys.getNodeText(tbRestServices.getRows()[tbRestServices.getIndex() - 1].businessObject, ".//*[local-name()='Resource']");
        var xmlStoreKey = resourceKeyPath + downloadResource + ".xml";
        if (!downloadResource || downloadResource == "")
        {
            application.notify(localization_get("selectDifferentInterface"));
        }
        else
        {
            downloadFile(xmlStoreKey, downloadResource);
        }
    }
}

function downloadFile(xmlStoreKey, fileName)
{
    var request = ID_download.XMLDocument;
    var xpath = globalObj.SOAPXPath + "/store:GetXMLObject/store:key";
    cordys.setXMLNamespaces(request, globalObj.nsPrefixes);
    cordys.setNodeText(request, xpath, xmlStoreKey);
    downloader.request = request;
    downloader.searchPath = "<tuple><old>";
    downloader.fileName = fileName + ".xml";
    downloader.openInNewWindow = "true";
    downloader.organization = application.organization.toLowerCase();
    downloader.dispositionType = "attachment";
    downloader.contentType = "text/xml";
    downloader.decode = false;
    downloader.downloadFile();
}


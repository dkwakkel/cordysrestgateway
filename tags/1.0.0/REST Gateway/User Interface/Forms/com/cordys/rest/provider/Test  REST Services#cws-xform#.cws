function captureRequestResponse(reqDoc, resDoc)
{
    sentDocList[sentDocCount] = createReqDocXML(reqDoc);
    clear.show();
    responseTable.show();
    prepareXML("sent", sentDocCount);
    sentDocCount += 1;
    responseDoc = cordys.loadXMLDocument(taResponse.getValue());
    replyDocList[replyDocCount] = responseDoc;
    prepareXML("received", replyDocCount);
    replyDocCount += 1;
    responseTable.setIndex(responseTable.getRows().length);
}

function createReqDocXML(reqDoc)
{
    var xmlDoc = cordys.getXMLDocument();
    var rootXml = xmlDoc.createElement("requestinfo");
    var reqNode = cordys.cloneXMLDocument(reqXML.XMLDocument).documentElement;
    cordys.appendXMLNode(reqNode, rootXml);
    cordys.setNodeText(reqNode, ".//URL", reqDoc);
    return rootXml;
}

function prepareXML(message, incrementor)
{
    var curDate = new Date();
    var date = curDate.toLocaleString();
    var xmlDoc = cordys.getXMLDocument();
    serialNo += 1;
    var rsMessage;
    if (message == "sent")
    {
        rsMessage = "Request Message" + (incrementor + 1);
    }
    else
    {
        rsMessage = "Response Message" + (incrementor + 1);
    }
    if (serialNo == 1)
    {
        rootXml = xmlDoc.createElement("resultmessage");
    }
    var elementNode = cordys.cloneXMLDocument(resultMessageXML.XMLDocument).documentElement;
    cordys.appendXMLNode(elementNode, rootXml);
    cordys.setNodeText(elementNode, ".//number", serialNo);
    cordys.setNodeText(elementNode, ".//time", date);
    cordys.setNodeText(elementNode, ".//type", message);
    cordys.setNodeText(elementNode, ".//message", rsMessage);
    ResultMessagesModel.clear();
    ResultMessagesModel.putData(rootXml, true);
    ResultMessagesModel.reset();
}

function clear_Click(eventObject)
{
    responseTable.redraw();
    responseTable.hide();
    clear.hide();
    resetCounters();
}

function resetCounters()
{
    sentDocList.slice(0);
    replyDocList.slice(0);
    replyDocCount = 0;
    sentDocCount = 0;
    serialNo = 0;
}

function responseTable_OnSelectRow(eventObject)
{
    var busObject = cordys.getEventTarget(eventObject).businessObject;
    var type = cordys.getNodeText(busObject, "*[local-name()='type']");
    var msg = cordys.getNodeText(busObject, "*[local-name()='message']");
    var number;
    if (type == "sent")
    {
        number = msg.substring(15);
    }
    else
    {
        number = msg.substring(16);
    }
    showMessages(msg, number, type);
}

function showMessages(msg, number, type)
{
    var messageViewerApp = cordys.cloneXMLDocument(messageViewerApplication.XMLDocument).documentElement;
    var idNode = cordys.selectXMLNode(messageViewerApp, "*[local-name()='id']");
    cordys.setTextContent(idNode, cordys.getTextContent(idNode) + msg);
    cordys.setNodeText(messageViewerApp, "*[local-name()='description']", msg);

    var update = new Object();
    if (type == "sent")
    {
        update.object = sentDocList[number - 1];
    }
    else
    {
        update.object = replyDocList[number - 1];
    }
    application.select(messageViewerApp, update, null);
}

